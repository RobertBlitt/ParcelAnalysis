    "# STEP 5: CREATE SUMMARY STATISTICS\n",
    "log_workflow_step(\"Master Table\", \"Generating summary statistics\")\n",
    "\n",
    "# Technology compatibility summary\n",
    "tech_summary = {}\n",
    "tech_fields = [\"MATRIX_SEPTIC_OK\", \"MATRIX_ATU_OK\", \"MATRIX_SEEPAGE_PIT_OK\"]\n",
    "\n",
    "for field in tech_fields:\n",
    "    compatible_count = 0\n",
    "    with arcpy.da.SearchCursor(master_table_path, [field]) as cursor:\n",
    "        for row in cursor:\n",
    "            if row[0] == 1:\n",
    "                compatible_count += 1\n",
    "    \n",
    "    tech_name = field.replace(\"MATRIX_\", \"\").replace(\"_OK\", \"\").replace(\"_\", \" \").title()\n",
    "    tech_summary[tech_name] = {\n",
    "        \"compatible\": compatible_count,\n",
    "        \"percentage\": (compatible_count / master_count) * 100\n",
    "    }\n",
    "\n",
    "print(\"\\n=== TECHNOLOGY COMPATIBILITY SUMMARY ===\")\n",
    "for tech, stats in tech_summary.items():\n",
    "    print(f\"{tech}: {stats['compatible']:,} parcels ({stats['percentage']:.1f}%)\")\n",
    "\n",
    "# Multi-technology compatibility\n",
    "multi_tech_count = 0\n",
    "no_tech_count = 0\n",
    "\n",
    "with arcpy.da.SearchCursor(master_table_path, tech_fields) as cursor:\n",
    "    for row in cursor:\n",
    "        compatible_techs = sum([1 for val in row if val == 1])\n",
    "        if compatible_techs > 1:\n",
    "            multi_tech_count += 1\n",
    "        elif compatible_techs == 0:\n",
    "            no_tech_count += 1\n",
    "\n",
    "multi_tech_pct = (multi_tech_count / master_count) * 100\n",
    "no_tech_pct = (no_tech_count / master_count) * 100\n",
    "\n",
    "print(f\"\\nMultiple technologies suitable: {multi_tech_count:,} ({multi_tech_pct:.1f}%)\")\n",
    "print(f\"No suitable technologies: {no_tech_count:,} ({no_tech_pct:.1f}%)\")"
